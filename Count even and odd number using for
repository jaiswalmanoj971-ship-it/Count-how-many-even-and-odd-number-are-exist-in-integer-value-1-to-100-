 #include <stdio.h>
 int main()
{

    int num;
    printf(" Enter a number:");
    scanf("%d",&num);
int even=0,odd=0;

for(int i=1;i<=num;i++){


    if(i%2==0){
        even++;
    }
        else{ 
            odd++;
        }
    }


printf("Even numbers: %d\n", even);
printf("Odd numbers: %d\n", odd);




return 0;
}   